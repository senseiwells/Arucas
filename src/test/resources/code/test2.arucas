debug(true);

experimental(true);
lazyValues = [];
retVal = [];
i = 0;
foreach (a : range(8)){
    _i = i;
    lambdaAsync = fun (){
        print("started Thread " + _i+ " at "+ getMilliTime());
        result = a;
        print("finished Thread " + _i+ " at "+ getMilliTime());
        return result;
    };
    lazyValues.append(Thread.runAsync(lambdaAsync));
    print("finished async call " + i + " at " + getMilliTime());
    i ++;
}
foreach(values : lazyValues){
    retVal.append(values);
}
print(retVal);
